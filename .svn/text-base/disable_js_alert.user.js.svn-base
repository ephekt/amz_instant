// ==UserScript==
// @name           disable js alert
// @namespace      osd
// @description    disable js alert
// ==/UserScript==

unsafeWindow.alert = function alert(message) {
	
		console.log(message);
	
}

unsafeWindow.onunload = function onunload(message) {
	
		console.log(message);
	
}

unsafeWindow.onbeforeunload = function onbeforeunload(message) {
	
		console.log(message);
	
}

unsafeWindow.beforeunload = function beforeunload(message) {
	console.log(message);
}

unsafeWindow.confirm = function confirm(message) {
	
		console.log(message);
	
}

unsafeWindow.prompt = function prompt(message) {
	
		console.log(message);
	
}

function pop_up() {};
